    <validationRules>
        <fullName>Lock_appeal_record_after_case_closure</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISNEW()), 
ISPICKVAL( Problem__r.Case__r.Status , &quot;Closed&quot;) 
)</errorConditionFormula>
        <errorMessage>You cannot update the appeal record once the related case has been closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Plan_required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Plan__c , &quot;&quot;)
&amp;&amp;
OR(
   ISPICKVAL( Grievance_Against_Plan__c ,&quot;Yes&quot;),
   ISPICKVAL( Informal_Review_with_Plan__c ,&quot;Yes&quot;)
)</errorConditionFormula>
        <errorDisplayField>Plan__c</errorDisplayField>
        <errorMessage>Plan is required</errorMessage>
    </validationRules>
